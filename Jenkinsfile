pipeline {
    agent any
    
    stages {
        stage('Build') {
            steps {
                echo 'This s the build stage'
            }
        }
        stage('Test') {
            steps {
                echo 'This is the test stage'
            }
        }
        stage('Deploy') {
            steps {
                echo 'This is the deployment stage'
            }
        }
    }
}
